class Solution {
    public int subarraySum(int[] arr, int target) {
        int sum=0;
        int ans=0;
        HashMap<Integer,Integer> map=new HashMap<>();
        map.put(0,1);
        for(int i=0;i<arr.length;i++)
        {
            sum=sum+arr[i];
            int rem=sum-target;
            if(map.containsKey(rem))
                ans=ans+map.get(rem);
        
                map.put(sum,map.getOrDefault(sum,0)+1);
        }
        return ans;
        
    }
}
